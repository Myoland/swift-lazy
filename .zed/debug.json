[
  {
    "label": "Debug Swift Test",
    "adapter": "CodeLLDB",
    "program": "/Applications/Xcode-16.2.0.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/libexec/swift/pm/swiftpm-testing-helper",
    "request": "launch",
    "cwd": "$ZED_WORKTREE_ROOT",
